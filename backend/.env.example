# App
APP_ENV=dev
PORT=8080
LOG_LEVEL=info
REQUEST_ID_HEADER=X-Request-Id

# Database (Postgres)
# Local
DATABASE_URL=postgres://inbota:inbota@localhost:5432/inbota?sslmode=disable
# Supabase (example)
# DATABASE_URL=postgres://postgres:YOUR_PASSWORD@db.<project-ref>.supabase.co:5432/postgres?sslmode=require

# Auth (JWT)
JWT_SECRET=

# AI Provider
AI_PROVIDER=groq
AI_API_KEY=
AI_BASE_URL=https://api.groq.com/openai/v1/chat/completions
AI_MODEL=llama-3.3-70b-versatile
AI_FALLBACK_MODEL=llama-3.1-8b-instant
AI_FALLBACK_ON_NEEDS_REVIEW=false
AI_TIMEOUT=15s
AI_MAX_RETRIES=2

# Timeouts
READ_TIMEOUT=5s
WRITE_TIMEOUT=10s
IDLE_TIMEOUT=60s
